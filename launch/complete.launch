<launch>    
    
  <node name="SLAM_sensor" pkg="SLAM_sensor" type="SLAM_sensor" output="screen"/>

  <node name="octomap_server_node" pkg="octomap_server" type="octomap_server_node" output="screen">
    <param name="sensor_model/max_range" value="5" />
		<param name="resolution" value="0.1" />
		<param name="latch" value="false" />
    <remap from="cloud_in" to="/points2" />
  </node>

  <node ns="" name="stereo_image_proc" pkg="stereo_image_proc" stereo_image_proc" output="screen">
    <param name="min_disparity" value="3" />
    <param name="disparity_range" value="32"/>
    <param name="speckle_size" value="531"/>
    <param name="speckle_range" value="8"/>
  </node>
</launch>
